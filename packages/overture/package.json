{
  "name": "overture",
  "version": "0.0.1",
  "description": "Yet another typescript prelude.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build-es": "tsc -p ./tsconfig.prod.json --outDir './dist/es' --module es2015",
    "build-umd": "tsc -p ./tsconfig.prod.json",
    "typecheck": "tsc -p . --noEmit",
    "lint": "standard 'src/**/*.ts'",
    "test": "jest"
  },
  "keywords": [
    "prelude"
  ],
  "author": "Jakub Kopa≈Ñski <jkopanski@protonmail.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "fast-check": "^1.16.0",
    "jest": "^24.8.0",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "typescript-language-server": "^0.3.8"
  },
  "dependencies": {
    "tshkt": "jkopanski/tshkt#241a34b"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
